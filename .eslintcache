/Home/Free/styles.ts",[],"/home/rafael/Desenvolvimento/Spotify-clone/src/pages/Layout/index.tsx",[],"/home/rafael/Desenvolvimento/Spotify-clone/src/routes/index.tsx",[],["126","127","128","129","130"],"/home/rafael/Desenvolvimento/Spotify-clone/src/components/Player/Menu/styles.ts",[],"/home/rafael/Desenvolvimento/Spotify-clone/src/components/Player/index.tsx",["131"],"import React, { useState } from 'react';\nimport * as M from '@material-ui/core';\nimport Header from './Header';\n\nimport Content from './Content';\n\nimport Menu from './Menu';\n\nimport Control from './Control';\n\nimport * as S from './styles';\n\nimport { albums } from './mocks/content';\n\nconst Player: React.FC = () => {\n    const [songs, _] = useState(albums);\n    return (\n        <>\n            <S.Container>\n                <Menu />\n                <S.Content>\n                    <Header />\n                    <S.TitleContent>\n                        <S.Title>Atalhos</S.Title>\n                        <S.SubTitle>Ver Tudo</S.SubTitle>\n                    </S.TitleContent>\n                    <M.Grid container>\n                        {songs.map((song) => (\n                            <Content\n                                key={song.id}\n                                image={song.image}\n                                title={song.title}\n                                description={song.description}\n                            />\n                        ))}\n                    </M.Grid>\n                </S.Content>\n            </S.Container>\n            <S.Player>\n                <Control />\n            </S.Player>\n        </>\n    );\n};\n\nexport default Player;\n","/home/rafael/Desenvolvimento/Spotify-clone/src/components/Player/Menu/index.tsx",[],["132","133","134","135","136"],"/home/rafael/Desenvolvimento/Spotify-clone/src/components/Player/Content/index.tsx",[],"/home/rafael/Desenvolvimento/Spotify-clone/src/components/Player/styles.ts",[],"/home/rafael/Desen